n                await this.$ajax\r\n                .get(`/test/son?${getInfo}`)\r\n                .then(res=>{console.log(res.data);this.inputarea=res.data})\r\n                .catch(err=>{console.log(err)});\r\n            }else{\r\n                let getInfo={}\r\n                this.get_key_value.forEach((value,index,array)=>{\r\n                    getInfo[value.key]=value.value;\r\n                })\r\n                console.log(getInfo);\r\n                await this.$ajax\r\n                .post('/test/son/',getInfo)\r\n                .then(res=>{console.log(res.data);this.inputarea=res.data})\r\n                .catch(err=>{console.log(err)});\r\n            }\r\n\r\n        },\r\n        async sendUser(type){\r\n            await this.$ajax\r\n            .get('test/configuser/')\r\n            .then(res=>{console.log(res.data)})\r\n            .catch(err=>{console.log(err)})\r\n        },\r\n\r\n    }\r\n    \r\n}\r\n</script>\r\n<style lang=\"css\">\r\n    \r\n</style>"],"mappings":";AAsDA,SAAAA,MAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,MAAA;MACAC,aAAA;QAAAC,EAAA,EAAAL,MAAA;QAAAM,GAAA;QAAAC,KAAA;MAAA;MACAC,SAAA;IACA;EACA;EACAC,OAAA;IACAC,MAAAC,IAAA;MACA,MAAAC,GAAA,QAAAR,aAAA,CAAAS,MAAA;MACA,IAAAF,IAAA;QACA,KAAAP,aAAA,CAAAU,IAAA;UAAAT,EAAA,EAAAL,MAAA;UAAAM,GAAA,MAAAM,GAAA;UAAAL,KAAA,MAAAK,GAAA;QAAA;MACA;QACA,KAAAR,aAAA,CAAAW,GAAA;MACA;MACA;MACA;MACA,KAAAC,SAAA;QACA,KAAAC,KAAA,cAAAb,aAAA,CAAAQ,GAAA,MAAAP,EAAA,OAAAa,KAAA;MACA;IACA;IACA,MAAAC,KAAA;MACA,SAAAhB,MAAA;QACA,IAAAiB,OAAA;QACA,KAAAhB,aAAA,CAAAiB,OAAA,EAAAd,KAAA,EAAAe,KAAA,EAAAC,KAAA;UACAH,OAAA,OAAAb,KAAA,CAAAD,GAAA,IAAAC,KAAA,CAAAA,KAAA;QACA;QACAa,OAAA,GAAAA,OAAA,CAAAI,KAAA,IAAAJ,OAAA,CAAAP,MAAA;QACAY,OAAA,CAAAC,GAAA,CAAAN,OAAA;QACA,WAAAO,KAAA,CACAC,GAAA,cAAAR,OAAA,IACAS,IAAA,CAAAC,GAAA;UAAAL,OAAA,CAAAC,GAAA,CAAAI,GAAA,CAAA5B,IAAA;UAAA,KAAAM,SAAA,GAAAsB,GAAA,CAAA5B,IAAA;QAAA,GACA6B,KAAA,CAAAC,GAAA;UAAAP,OAAA,CAAAC,GAAA,CAAAM,GAAA;QAAA;MACA;QACA,IAAAZ,OAAA;QACA,KAAAhB,aAAA,CAAAiB,OAAA,EAAAd