dasfdasfa\",\r\n            all_content2:\"dafdasfdasfa\",\r\n            num:0,\r\n            cursor:\"\",\r\n        }\r\n    },\r\n    methods: {\r\n        show(intro,content){\r\n            return new Promise((resolve,reject)=>{\r\n                this.interval = setInterval(()=>{\r\n                    this.cursor = \"animate__animated animate__flash animate__infinite\"\r\n                    if (this.num > content.length){\r\n                        clearInterval(this.interval);\r\n                        setTimeout(()=>{\r\n                            resolve();\r\n                        },1000)\r\n                    }\r\n                    this.intro += content.slice(this.num,this.num+1);\r\n                    this.num += 1;\r\n                },200)\r\n            })\r\n        },\r\n\r\n        oprmove(idx){\r\n            if(idx === 1){\r\n                this.cursor += \" move\";\r\n            }else if(idx === 2){\r\n                this.cursor = \"animate__animated animate__flash animate__infinite move2\";\r\n            }\r\n        }\r\n    },\r\n\r\n    async mounted() {\r\n        await this.show(this.intro,this.all_content);\r\n        await this.oprmove(1);\r\n        setTimeout(()=>{\r\n            this.oprmove(2);\r\n        },1500);\r\n    },\r\n}\r\n</script>\r\n<style lang=\"css\" scoped>\r\n    .title_style{\r\n        font-size:60px;\r\n        font-weight:bold;\r\n        display: inline-block;\r\n    }\r\n    .move{\r\n        animation: m 1s 1;\r\n        animation-fill-mode: forwards;\r\n    }\r\n    .move2{\r\n        transform: translate(-390px,70px);\r\n    }\r\n    @keyframes m{\r\n        100%{\r\n            transform: translate(-390px,70px);\r\n        }\r\n    }\r\n</style>"],"mappings":"AAWA;AACA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,KAAA;MACAC,WAAA;MACAC,YAAA;MACAC,GAAA;MACAC,MAAA;IACA;EACA;EACAC,OAAA;IACAC,KAAAN,KAAA,EAAAO,OAAA;MACA,WAAAC,OAAA,EAAAC,OAAA,EAAAC,MAAA;QACA,KAAAC,QAAA,GAAAC,WAAA;UACA,KAAAR,MAAA;UACA,SAAAD,GAAA