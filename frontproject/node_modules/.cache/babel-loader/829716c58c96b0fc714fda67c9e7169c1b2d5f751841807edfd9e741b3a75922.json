{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport store from \"../../store\";\nimport \"animate.css\";\nexport default {\n  name: \"Sup\",\n  beforeRouteEnter(to, from, next) {\n    store.state.divWid = 590;\n    next();\n  },\n  data() {\n    return {\n      userInfo: {\n        username: \"dd\",\n        password: \"dd\",\n        email: \"dd\",\n        country: \"China\",\n        phone: \"\",\n        organization: \"\"\n      },\n      drop: \"gmail.com\",\n      step: 0,\n      tostyle: \"el-icon-s-promotion\"\n    };\n  },\n  methods: {\n    handlecom(type) {\n      this.drop = type;\n    },\n    handlecom2(type) {\n      this.userInfo.country = type;\n    },\n    hdenter(idx) {\n      this.inputList[idx + 1].focus();\n    },\n    querySearch(queryString, cb) {\n      let restaurants = this.restaurants;\n      let results = queryString ? restaurants.filter(this.createFilter(queryString)) : restaurants;\n      // 调用 callback 返回建议列表的数据\n      cb(results);\n    },\n    createFilter(queryString) {\n      return restaurant => {\n        return restaurant.value.toLowerCase().indexOf(queryString.toLowerCase()) === 0;\n      };\n    },\n    loadAll() {\n      let loadarray = [];\n      this.$ajax.get(\"media/load/value-key.json\").then(res => {\n        console.log(res.data[\"0\"]);\n        for (let i = 0; i < Object.keys(res.data).length; i++) {\n          loadarray.push(res.data[`${i}`]);\n        }\n      }).catch(err => {\n        console.log(err);\n      });\n      return loadarray;\n    },\n    moved() {\n      this.tostyle = \"el-icon-s-promotion animate__animated animate__fadeInBottomRight\";\n    }\n  },\n  mounted() {\n    this.inputList = [this.$refs.input1, this.$refs.input2, this.$refs.input3];\n    this.restaurants = this.loadAll();\n  }\n};","map":{"version":3,"names":["store","name","beforeRouteEnter","to","from","next","state","divWid","data","userInfo","username","password","email","country","phone","organization","drop","st