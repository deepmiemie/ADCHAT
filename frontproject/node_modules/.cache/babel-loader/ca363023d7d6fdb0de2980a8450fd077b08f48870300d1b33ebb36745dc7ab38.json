         getInfo+=`${value.key}=${value.value}&`\r\n                })\r\n                getInfo=getInfo.slice(0,getInfo.length-1)\r\n                console.log(getInfo)\r\n                await this.$ajax\r\n                .get(`/test/son?${getInfo}`)\r\n                .then(res=>{console.log(res.data);this.inputarea=res.data})\r\n                .catch(err=>{console.log(err)});\r\n            }else{\r\n                let getInfo={}\r\n                this.get_key_value.forEach((value,index,array)=>{\r\n                    getInfo[value.key]=value.value;\r\n                })\r\n                console.log(getInfo);\r\n                await this.$ajax\r\n                .post('/test/son/',getInfo)\r\n                .then(res=>{console.log(res.data);this.inputarea=res.data})\r\n                .catch(err=>{console.log(err)});\r\n            }\r\n\r\n        },\r\n        async sendUser(type){\r\n            if(type===\"get\"){\r\n                await this.$ajax\r\n                .get('test/configuser/')\r\n                .then(res=>{\r\n                    console.log(res.data);\r\n                    this.tableData=res.data;\r\n                })\r\n                .catch(err=>{console.log(err)})\r\n            }else if(type==='post'){\r\n                await this.$ajax\r\n                .post('test/configuser/',this.userInfo)\r\n                .then(res=>{\r\n                    console.log(res.data)\r\n                })\r\n                .catch(err=>{console.log(err)})\r\n            }\r\n\r\n\r\n        },\r\n\r\n    }\r\n    \r\n}\r\n</script>\r\n<style lang=\"css\">\r\n    \r\n</style>"],"mappings":";AAyEA,SAAAA,MAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,MAAA;MACAC,aAAA;QAAAC,EAAA,EAAAL,MAAA;QAAAM,GAAA;QAAAC,KAAA;MAAA;MACAC,SAAA;MACAC,SAAA;MACAC,QAAA;QAAAL,EAAA;QAAAM,QAAA;QAAAC,SAAA;MAAA;IACA;EACA;EACAC,OAAA;IACAC,MAAAC,IAAA;MACA,MAAAC,GAAA,QAAAZ,aAAA,CAAAa,MAAA;MACA,IAAAF,IAAA;QACA,KAAAX,aAAA,CAAAc,IAAA;UAAAb,EAAA,EAAAL,MAAA;UAAAM,GAAA,MAAAU,GAA