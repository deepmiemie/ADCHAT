{"ast":null,"code":"export default {\n  name: \"Mychat\",\n  data() {\n    return {\n      textcontent: \"\",\n      gptcontent: \"\",\n      gptcontent_in: \"\",\n      isLoading: false,\n      isStopping: false,\n      num: 0,\n      asictt: \"\",\n      url: \"https://pic.52112.com/2019/06/06/JPS-190606_155/24poJOgl7m_small.jpg\",\n      srcList: [\"https://pic.52112.com/2019/06/06/JPS-190606_155/24poJOgl7m_small.jpg\"]\n    };\n  },\n  methods: {\n    async geText() {\n      console.log('您正在向chatgpt发送内容...');\n      let num = 0;\n      await this.$ajax.get(`/test/getchat?question=${this.textcontent}&isinit=true`).then(res => {\n        console.log(res.data.data);\n        this.gptcontent = res.data.data;\n      }).catch(err => {\n        console.log(err);\n      });\n      this.isLoading = false;\n      this.isStopping = false;\n      this.interval = setInterval(() => {\n        if (!this.isStopping) {\n          this.$ajax.get(`/test/getchat?question=${this.textcontent}&isinit='false'`).then(res => {\n            this.gptcontent_in += res.data.data;\n            this.isLoading = true;\n          }).catch(err => {\n            this.isStopping = true;\n            console.log(err);\n          });\n        }\n        if (this.isLoading) {\n          const icontent = this.gptcontent_in.slice(this.num, this.num + 1);\n          if (icontent === \"*****stop*****\") {\n            clearInterval(this.interval);\n          }\n          this.gptcontent += icontent;\n          this.num++;\n        }\n      }, 100);\n    }\n  },\n  computed: {\n    asictt_length() {\n      return 40 + this.asictt.length * 15;\n    }\n  }\n};","map":{"version":3,"names":["name","data","textcontent","gptcontent","gptcontent_in","isLoading","isStopping","num","asictt","url","srcList","methods","geText","console","log","$ajax","get","then","res","catch","err","interval","setInterval","icontent","slice","clearInterval","computed","asictt_length","length"],"sources":["src/components/Mychat.vue"]